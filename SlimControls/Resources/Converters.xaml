    <slimControls:ConstantStringExtension Key="TextureNoise" x:Key="TextureNoise" />
    <slimControls:ConstantStringExtension Key="TextureClouds" x:Key="TextureClouds" />
    <slimControls:ConstantStringExtension Key="TextureMarble" x:Key="TextureMarble" />
    <slimControls:ConstantStringExtension Key="TextureWood" x:Key="TextureWood" />
    <slimControls:ConstantStringExtension Key="TextureWave" x:Key="TextureWave" />
    <slimControls:ConstantStringExtension Key="TextureCrosshatch" x:Key="TextureCrosshatch" />
    <slimControls:ConstantStringExtension Key="TextureConcrete" x:Key="TextureConcrete" />
    <slimControls:ConstantStringExtension Key="TextureCanvas" x:Key="TextureCanvas" />

