    <!-- Selection Tool Strings -->
    <slimControls:ConstantStringExtension Key="RectangleText" x:Key="RectangleText" />
    <slimControls:ConstantStringExtension Key="EllipseText" x:Key="EllipseText" />
    <slimControls:ConstantStringExtension Key="FreeFormText" x:Key="FreeFormText" />

